{
  "$schema": ".yarn/unplugged/nx-virtual-62b135742d/node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "nxCloudId": "673a16f83f47fa52b6b6deac",
  "plugins": [
    "@nx-dotnet/core",
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "build",
        "testTargetName": "test",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "serveStaticTargetName": "serve-static"
      }
    }
  ],
  "projects": {
    "SecureWeb": {
      "root": "SecureWeb"
    },
    "SecureWeb-e2e": {
      "root": "SecureWeb-e2e"
    }
  },
  "tasks": {
    "nx-dotnet:restore": {
      "executor": "@nx-dotnet/core:restore"
    },
    "nx-dotnet:build": {
      "executor": "@nx-dotnet/core:build"
    },
    "nx-dotnet:test": {
      "executor": "@nx-dotnet/core:test"
    },
    "nx-dotnet:clean": {
      "executor": "@nx-dotnet/core:clean"
    }

  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "nx-dotnet:restore"
      ]
    }
  }
}
